{
  "name": "f2e-server3",
  "version": "1.7.0-alpha",
  "description": "f2e-server 3.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "src/index.ts",
  "files": [
    "lib",
    "templates",
    "src"
  ],
  "bin": {
    "f2e3": "lib/f2e.js"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.lib.json",
    "test": "npm run tsc && node ./test/start.cjs",
    "test:bun": "npm run tsc && bun ./test/start.cjs",
    "test:deno": "npm run tsc && deno run --allow-env --allow-read --allow-write --allow-net --allow-run --allow-sys ./test/start.cjs",
    "build": "npm run tsc && node ./test/start.cjs -m build",
    "build:bun": "npm run tsc && node ./test/start.cjs",
    "prepublishOnly": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git@gitee.com:f2e-server/f2e-server-3.git"
  },
  "keywords": [
    "f2e-server"
  ],
  "author": "shy2850",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/deno": "^2.0.0",
    "@types/less": "^3.0.6",
    "@types/node": "^22.7.5",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "antd": "^5.16.4",
    "chokidar": "^3.6.0",
    "esbuild": "^0.20.2",
    "esbuild-plugin-postcss": "^0.2.1",
    "highlight.js": "^11.9.0",
    "less": "^4.2.0",
    "marked": "^12.0.1",
    "mime": "^4.0.3",
    "postcss": "^8.4.41",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.4.5",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.43.0"
  }
}
